@model IEnumerable<GearGauge.Models.Favorites>

<h2>Your Favorites</h2>

<ul class="list-group">
    @foreach (var favorite in Model)
    {
        <li class="list-group-item">
            <h4>@favorite.GearInventory.Title</h4>
            <p>@favorite.GearInventory.Description</p>
            
        </li>
    }
</ul>
